enum UserRole {
  ADMIN
  USER
}

model User {
  id        String   @id @default(cuid())
  name      String
  role      UserRole @default(USER)
  email     String   @unique
  password  String
  document  String   @unique
  phone     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}
